{
    "name": "lcherone/php-petname",
    "type": "library",
    "description": "Library for generating human-readable, random names for objects.",
    "license": "MIT",
    "keywords": [
        "library",
        "generate",
        "petname",
        "petnames",
        "hostname",
        "random",
        "names"
    ],
    "homepage": "https://github.com/lcherone/php-petname",
    "authors": [
        {
            "name": "Lawrence Cherone",
            "email": "lawrence@cherone.co.uk",
            "homepage": "https://github.com/lcherone/php-petname",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "LCherone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LCherone\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --coverage-text"
    },
    "minimum-stability": "stable"
}
